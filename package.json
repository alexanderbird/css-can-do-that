{
  "name": "css-can-do-that",
  "version": "0.0.1",
  "description": "Can you make a multi-page website with a single html file and CSS (no JavaScript)? Here's a proof of concept.",
  "main": ".html/index.html",
  "dependencies": {
    "s3-static-website-deploy": "^1.0.0"
  },
  "devDependencies": {
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-html-beautify": "^1.0.1",
    "gulp-wrap": "^0.14.0",
    "http-server": "^0.11.1"
  },
  "scripts": {
    "deploy": "s3-static-website-deploy --domain nojs.sandbox.alexanderbird.software --region ca-central-1 --directory .html",
    "build": "gulp",
    "start": "cd .html && http-server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
